{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A collection of encumbrances",
  "type": "object",
  "properties": {
    "encumbrances": {
      "description": "The list of encumbrance objects in this collection",
      "type": "array",
      "id": "encumbrances",
      "items": {
        "description": "The encumbrance items",
        "type": "object",
        "$ref": "encumbrance.json"
      }
    },
    "totalRecords": {
      "description": "The number of objects in this collection",
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "required": [
    "encumbrances",
    "totalRecords"
  ]
}
