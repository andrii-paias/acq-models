{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "collection of alias records",
    "type": "object",
    "properties": {
      "aliases": {
        "description": "The list of aliases in this collection",
        "type": "array",
        "id": "aliases",
        "items": {
          "type": "object",
          "$ref": "alias.json"
        }
      },
      "totalRecords": {
        "description": "The number of alias records returned in this collection",
        "type": "integer"
      }
    },
    "additionalProperties": false,
    "required": [
      "aliases",
      "totalRecords"
    ]
  }
