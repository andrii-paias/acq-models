{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Collection of interface credential records",
    "type": "object",
    "properties": {
      "interfaceCredentials": {
        "description": "The list of interface credentials in this collection",
        "type": "array",
        "id": "interfaceCredentials",
        "items": {
          "type": "object",
          "$ref": "interface_credential.json"
        }
      },
      "totalRecords": {
        "description": "The number of interface credential records returned in this collection",
        "type": "integer"
      }
    },
    "additionalProperties": false,
    "required": [
      "interfaceCredentials",
      "totalRecords"
    ]
  }
