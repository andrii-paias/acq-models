{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "POLine number object",
  "type": "object",
  "properties": {
    "sequenceNumber": {
      "description": "POLine number",
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{1,3}$"
    }
  },
  "required": [
    "sequenceNumber"
  ],
  "additionalProperties": false
}
